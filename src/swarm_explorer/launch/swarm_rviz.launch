<launch>
  <!-- Use sim time -->
  <param name="use_sim_time" value="true"/>
  <param name="/use_sim_time" value="true"/>

  <!-- Launch RViz with the swarm configuration -->
  <node pkg="rviz" type="rviz" name="rviz" 
        args="-d $(find swarm_explorer)/config/swarm_w_occupancy_grid.rviz"/>
</launch> 